{
  "name": "export-folder-to-index",
  "displayName": "export-folder-to-index",
  "description": "Create './index.ts' file that exports the file included in the directory.",
  "version": "0.1.5",
  "publisher": "kareemalkoul",
  "repository": {
    "type": "git",
    "url": "https://github.com/kareemalkoul/export-folder-to-index.git"
  },
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.addCurrentFileExportationToIndex",
    "onCommand:extension.addCurrentFolderExportationToIndex"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.addCurrentFileExportationToIndex",
        "title": "Add export declaration of current file to index.js or index.ts"
      },
      {
        "command": "extension.addCurrentFolderExportationToIndex",
        "title": "Add export declaration of current folder to index.js or index.ts"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.60",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "vscode": "^1.1.26"
  }
}
